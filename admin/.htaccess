# 静态页面路由重定向
RewriteEngine On

# 重定向根路径到index.html
RewriteRule ^$ index.html [L]

# 重定向页面路由到对应的HTML文件
RewriteRule ^dashboard$ pages/dashboard.html [L]
RewriteRule ^categories$ pages/categories.html [L]
RewriteRule ^attractions$ pages/attractions.html [L]
RewriteRule ^articles$ pages/articles.html [L]
RewriteRule ^map-editor$ pages/map-editor.html [L]
RewriteRule ^settings$ pages/settings.html [L]

# 重定向CSS和JS文件
RewriteRule ^css/(.*)$ css/$1 [L]
RewriteRule ^js/(.*)$ js/$1 [L]
RewriteRule ^pages/(.*)$ pages/$1 [L]